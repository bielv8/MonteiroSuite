modules = ["web", "nodejs-20", "python-3.11"]

[nix]
channel = "stable-25_05"
packages = ["openssl", "postgresql"]

[deployment]
deploymentTarget = "autoscale"
run = ["gunicorn", "--bind", "0.0.0.0:5000", "main:app"]

[workflows]

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 8080
externalPort = 8080

[[ports]]
localPort = 42615
externalPort = 3000

[agent]
integrations = ["python_log_in_with_replit==1.0.0", "python_database==1.0.0"]
expertMode = true
